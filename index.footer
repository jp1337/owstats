   <script language="javascript">

var myTextExtraction = function(node) 
{ 
    // extract data from markup and return it 
    return node.dataset.value; 
} 

    $(function() {
	$(".sortable").tablesorter( {textExtraction: myTextExtraction} );
	$(".chart").peity("bar", {
		fill: function(value) {
			return value > 0 ? "green" : "red"
		}
});
    });
   </script> 
  <div class="small">generated GENTIMESTRING</div>
</body>

</html>
