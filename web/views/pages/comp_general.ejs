<%- include('../includes/header'); -%>

<div id="page-container" class="sidebar-l sidebar-o side-scroll header-navbar-fixed">
    <%- include('../includes/main_sidebar', {url: url}); -%>

    <main id="main-container">
        <%- include('../includes/breadcrumb', {breadcrumb: breadcrumb}); -%>

        <div class="content content-narrow">
            <div class="row">
                <div class="col-lg-12">
                    <div class="block">
                        <div class="block-header">
                            <h3 class="block-title">Lines</h3>
                        </div>
                        <div class="block-content block-content-full text-center">
                            <canvas class="js-chartjs2-lines"></canvas>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <div class="block">
                        <div class="block-header">
                            <h3 class="block-title">Bars</h3>
                        </div>
                        <div class="block-content block-content-full text-center">
                            <canvas class="js-chartjs2-bars"></canvas>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </main>
    <%- include('../includes/main_footer', {footer: footer}); -%>
</div>

<%- include('../includes/footer', {chartjs: true}); -%>