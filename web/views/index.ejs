                            </li>
                            <li>
                                <a href="base_pages_dashboard.html"><i class="fa fa-pie-chart"></i><span class="sidebar-mini-hide">Average</span></a>
                            </li>
                            <li>
                                <a href="base_pages_dashboard.html"><i class="fa fa-area-chart"></i><span class="sidebar-mini-hide">Heroes</span></a>
                            </li>

                            <li class="nav-main-heading"><span class="sidebar-mini-hide">OWstats</span></li>

                            <li>
                                <a href="base_pages_dashboard.html"><i class="fa fa-bar-chart"></i><span class="sidebar-mini-hide">Database</span></a>
                            </li>

                        </ul>
                    </div>
                    <!-- END Side Content -->
                </div>
                <!-- Sidebar Content -->
            </div>
            <!-- END Sidebar Scroll Container -->
        </nav>
